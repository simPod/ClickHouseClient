{
    "name": "simpod/clickhouse-client",
    "type": "library",
    "description": "PHP ClickHouse Client",
    "keywords": [
        "clickhouse",
        "client",
        "php",
        "http",
        "HTTP client",
        "curl"
    ],
    "homepage": "https://github.com/simPod/ClickHouseClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Podlipsky",
            "email": "simon@podlipsky.net"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php-64bit": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "consistence/consistence": "^2.0",
        "guzzlehttp/promises": "^1.3",
        "guzzlehttp/psr7": "^1.4",
        "nette/utils": "^2.5",
        "psr/log": "^1.0",
        "thecodingmachine/safe": "^0.1.8"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0 || ^6.0",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "phpunit/phpunit": "^7",
        "thecodingmachine/phpstan-safe-rule": "^0.1.0"
    },
    "autoload": {
        "files": [
            "src/GuzzleHttp/functions_include.php"
        ],
        "psr-4": {
            "SimPod\\ClickHouseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPod\\ClickHouseClient\\Tests\\": "tests/"
        }
    }
}
